LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/ACEDrawingView" "$PODS_CONFIGURATION_BUILD_DIR/AFNetworking" "$PODS_CONFIGURATION_BUILD_DIR/Color-Picker-for-iOS" "$PODS_CONFIGURATION_BUILD_DIR/HMSegmentedControl" "$PODS_CONFIGURATION_BUILD_DIR/MBProgressHUD" "$PODS_CONFIGURATION_BUILD_DIR/NJKScrollFullScreen" "$PODS_CONFIGURATION_BUILD_DIR/OLGhostAlertView" "$PODS_CONFIGURATION_BUILD_DIR/PBJVision" "$PODS_CONFIGURATION_BUILD_DIR/SCRecorder"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/ACEDrawingView" -isystem "${PODS_ROOT}/Headers/Public/AFNetworking" -isystem "${PODS_ROOT}/Headers/Public/Color-Picker-for-iOS" -isystem "${PODS_ROOT}/Headers/Public/HMSegmentedControl" -isystem "${PODS_ROOT}/Headers/Public/MBProgressHUD" -isystem "${PODS_ROOT}/Headers/Public/NJKScrollFullScreen" -isystem "${PODS_ROOT}/Headers/Public/OLGhostAlertView" -isystem "${PODS_ROOT}/Headers/Public/PBJVision" -isystem "${PODS_ROOT}/Headers/Public/SCRecorder"
OTHER_LDFLAGS = $(inherited) -ObjC -l"ACEDrawingView" -l"AFNetworking" -l"Color-Picker-for-iOS" -l"HMSegmentedControl" -l"MBProgressHUD" -l"NJKScrollFullScreen" -l"OLGhostAlertView" -l"PBJVision" -l"SCRecorder" -framework "AVFoundation" -framework "CoreGraphics" -framework "CoreImage" -framework "CoreMedia" -framework "CoreVideo" -framework "Foundation" -framework "ImageIO" -framework "MobileCoreServices" -framework "OpenGLES" -framework "QuartzCore" -framework "Security" -framework "SystemConfiguration" -framework "UIKit" -weak_framework "GLKit" -weak_framework "Metal"
